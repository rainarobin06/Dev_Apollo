    <div class="modal"
         tabindex="-1"
         id="attachModal"
         role="dialog"
         aria-labelledby="attachModalLabel"
         aria-hidden="true">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <p ng-if="allAttachments.length > 0" class="cls-header m-0">
                        {{showTable.FormName}} Attachments ({{allAttachments.length}})
                    </p>
                    <button type="button"
                            class="close"
                            data-bs-dismiss="modal"
                            aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container-fluid">
                        <h3 class="h6">
                            Glimpses of activities for {{ReportDetails.HospitalName}} in the
                            month of {{ReportDetails.MonthName}} {{ReportDetails.Year}}
                        </h3>
                        <p class="text-secondary small">
                            Click on the file name to download
                        </p>
                        <ul class="cls-view-attachments cls-preview-attachment">
                            <li ng-repeat="file in allAttachments">
                                <img ng-if="file.file_ext == '.png' || file.file_ext == '.jpg' || file.file_ext == '.jpeg'"
                                     src="{{file.fileLink}}"
                                     alt="{{file.file_name}}" />
                                <img ng-if="file.file_ext != '.png' && file.file_ext != '.jpg' && file.file_ext != '.jpeg'"
                                     src="~/Content/icons/file_icon.png"
                                     alt="{{file.file_name}}" />
                                <a class="download-file1"
                                   href="{{file.fileLink}}"
                                   target="_blank">{{file.file_name}}</a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button"
                            class="btn btn-secondary"
                            data-bs-dismiss="modal">
                        Close
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>
