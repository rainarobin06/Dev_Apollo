@{ ViewBag.Title = "Report"; }

<div ng-app="nursingApp" ng-controller="reportController">
    <!--dropdown1-->
    <div class="row">
        <div class="col-8 offset-2">
            <div class="row mb-2">
                <label class="col-md-2">Region</label>
                <div class="col-md-8">
                    <oi-select oi-options="loc as loc for loc in MappedRegions"
                               ng-model="RegionSelected"
                               ng-change="ChangeRegion()"
                               placeholder="Select"
                               multiple
                               ng-disabled="MappedRegions.length == 0">
                    </oi-select>
                </div>
            </div>

            <div class="row mb-2">
                <label class="col-md-2">Location</label>
                <div class="col-md-8">
                    <oi-select oi-options="locList.HospitalName for locList in UserMappedHospitals"
                               ng-model="PlaceSelected"
                               placeholder="Select"
                               multiple
                               ng-disabled="UserMappedHospitals.length == 0">
                    </oi-select>
                </div>
            </div>

            <div class="row mb-2">
                <label class="col-md-2">Report</label>
                <div class="col-md-8">
                    <oi-select oi-options="repList.FormName for repList in report"
                               ng-model="ReportSelected"
                               placeholder="Select"
                               multiple
                               ng-change="CheckAllReport()"
                               ng-disabled="report.length == 0">
                    </oi-select>
                </div>
            </div>

            <div class="row mb-2">
                <label class="col-md-2">Start Month</label>
                <div class="col-md-3">
                    <div class="days">
                        <div class="col" style="margin-right: 3px">
                            <select class="form-select form-control"
                                    ng-change="setStartEndDate()"
                                    ng-model="StartMonthtSelected"
                                    ng-options="option.name for option in monthData track by option.value"></select>
                        </div>
                        <div class="col">
                            <select class="form-select form-control"
                                    ng-change="setStartEndDate()"
                                    ng-model="StartYearSelected"
                                    ng-options="option as option for option in yearData"></select>
                        </div>
                    </div>
                </div>
                <label class="col-md-2">End Month</label>
                <div class="col-md-3">
                    <div class="days">
                        <div class="col" style="margin-right: 3px">
                            <select class="form-select form-control"
                                    ng-change="setStartEndDate()"
                                    ng-model="EndMonthtSelected"
                                    ng-options="option.name for option in monthData track by option.value"></select>
                        </div>
                        <div class="col">
                            <select class="form-select form-control"
                                    ng-change="setStartEndDate()"
                                    ng-model="EndYearSelected"
                                    ng-options="option as option for option in yearData"></select>
                        </div>
                    </div>
                </div>
            </div>
            <p class="text-danger small" ng-if="StartDate > EndDate">
                End date should not be lesser than Start date.
            </p>
            <div class="row mb-2 mt-3">
                <div class="col-md-3 text-end"></div>
                <div class="col-md-2">
                    <button ng-click="viewReport()"
                            ng-disabled="PlaceSelected.length === 0 || ReportSelected.length === 0 || StartDate > EndDate"
                            type="button"
                            class="btn btn-warning w-75 shadow">
                        View
                    </button>
                </div>
                <div class="col-md-2">
                    <button ng-click="ExportToExcel()"
                            ng-disabled="PlaceSelected.length === 0 || ReportSelected.length === 0 || StartDate > EndDate || IsExporting"
                            type="button"
                            class="btn btn-warning w-75 shadow">
                        Export
                    </button>
                </div>
                <div class="col-md-2">
                    <button ng-click="ResetSelection()"
                            type="button"
                            class="btn btn-warning w-75 shadow">
                        Clear
                    </button>
                </div>
            </div>
            <p ng-if="IsExporting"
               class="text-success text-center alert-success accordion-body mt-5">
                Please wait...
            </p>
        </div>
    </div>

    <!-- View Report -->
    <div class="view-report">
        <ul class="cls-reporttab" ng-hide="tabsReportsSelected.length < 2">
            <li ng-repeat="report in tabsReportsSelected"
                ng-class="{'active': report.Formcode === showTable.Formcode}"
                ng-click="showTableData(report)">
                {{report.FormName}}
            </li>
        </ul>

        <p class="cls-header" ng-if="tabsReportsSelected.length > 0">
            {{showTable.FormName}} Report
            <span ng-if="StartMonthtSelected == EndMonthtSelected && StartYearSelected == EndYearSelected">for {{StartDate | date : 'MMM yyyy'}}</span>
            <span ng-if="StartMonthtSelected != EndMonthtSelected || StartYearSelected != EndYearSelected">
                between {{StartDate | date : 'MMM yyyy'}} and {{EndDate | date : 'MMM
        yyyy'}}
            </span>
        </p>

        <table class="table table-striped table-bordered"
               ng-show="showTable.Formcode == 'TBL01'"
               rel="COE">
            <thead>
                <tr>
                    <th>Units</th>
                    <th>Month</th>
                    <th>COE (Name)</th>
                    <th>Number of Nurses attended</th>
                    <th>Consultants involved</th>
                    <th>Nature of Activity Done</th>
                    <th>Consultant Feedback</th>
                    <th>Action taken Post Feedback</th>
                    <th>Any additional mentions</th>
                    <th>Remarks</th>
                    <th>Form Status</th>
                    <th>Submitted Date</th>
                    <th>Submitted By</th>
                    <th>Glimpses</th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="report in ReportData">
                    <td>{{report.HospitalName}}</td>
                    <td>{{report.MonthName}} {{report.Year}}</td>
                    <td>{{report.ReportName}}</td>
                    <td>{{report.NoOfNursesAttended}}</td>
                    <td>{{report.ConsultantsInvolved}}</td>
                    <td>{{report.NatureOfActivityDone}}</td>
                    <td>{{report.ConsultantFeedback}}</td>
                    <td>{{report.ActionTaken}}</td>
                    <td>{{report.AdditionalMentions}}</td>
                    <td>{{report.Remarks}}</td>
                    <td>{{report.FormStatus}}</td>
                    <td>{{report.Modified | date : 'dd-MMM-yyyy hh:mm a'}}</td>
                    <td>{{report.ModifiedBy}}</td>
                    <td>
                        <span ng-if="report.Attachments"
                              class="viewattach-link small"
                              data-bs-toggle="modal"
                              data-bs-target="#attachModal"
                              ng-click="viewAttachments(report)">View Attachments</span>
                        <p ng-if="report.Attachments" class="small p-0 m-0 text-center">
                            (or)
                        </p>
                        <a class="cls-report-attachments-link"
                           ng-if="report.Attachments.indexOf(';')==-1"
                           title="Click to download"
                           href="{{report.Attachments}}">Attachment-1</a>
                        <ul class="cls-report-attachments"
                            ng-if="report.Attachments.indexOf(';')>-1">
                            <li ng-repeat="_attach in report.Attachments | splitAttachments">
                                <a title="Click to download" href="{{_attach}}">Attachment-{{($index+1)}}</a>
                            </li>
                        </ul>
                    </td>
                </tr>
            </tbody>
        </table>

